    <!-- Sidebar right -->
    <nav id="sidebarleft" class="sidenav">
        <div id="dismiss">
            <i class="fas fa-times"></i>
        </div>
        <div class="sidebar-header">
            <h3>Let's eat!</h3>
        </div>
        <div class="search-form">
            <div class="search-wrapper ">
                <button class="ssubmit" onclick="search()" type="submit" name="search_submit"><i class="fas fa-search"></i></button>
                <input id="search">
            </div>
            @await Html.PartialAsync("_CategoriesPartial")
        </div>
    </nav>
    <!-- .Sidebar right-->
    <!-- Page content  -->
    <div id="content">
        <!-- Header  -->
        <nav class="navbar navbar-expand-lg navbar-light bg-header">
            <div class="container-fluid">
                <a href="#sidebarright" type="button" id="sidebarrightbutton" class="btn">
