    <!--  Custom JS -->
    <script>
        var cart_list;
        var CartItem = function () {
            this.Id = null,
                this.Name = null,
                this.Price = null,
                this.Count = null,
                this.Image = null,
                this.Description = null
        };
    </script>
    <script src="~/js/theme.js"></script>
    <script src="~/js/inner-content.js"></script>
    <script src="~/js/outer-content.js"></script>
    <script src="~/js/site.js"></script>
